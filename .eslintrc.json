{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "project": "tsconfig.json"
    },
    
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "settings": {
        "import/resolver":{
            "typescript": {}
        }
       
    },
    
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "quotes": "off",
        "import/prefer-default-export": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "ts": "never"
            }
         ]
    }
}
